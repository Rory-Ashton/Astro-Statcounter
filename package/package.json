  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "astro": "^4.0.0"
  },
  "peerDependencies": {
    "astro": "^4.0.0 || ^5.0.0"
  },
  "keywords": ["astro", "component", "analytics", "statcounter"]
}