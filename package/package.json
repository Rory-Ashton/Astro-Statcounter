{
  "name": "@statcounter/astro",
  "version": "0.1.1",
  "description": "Official Statcounter integration for Astro",
  "license": "MIT",
  "type": "module",
  "exports": {
  ".": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  }
},
